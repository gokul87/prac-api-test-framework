{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "firstname": {
      "$id": "/properties/firstname",
      "type": "string",
      "title": "The Firstname Schema ",
      "default": "",
      "examples": [
        "Mark"
      ]
    },
    "lastname": {
      "$id": "/properties/lastname",
      "type": "string",
      "title": "The Lastname Schema ",
      "default": "",
      "examples": [
